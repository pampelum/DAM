package ex_456;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int t, w, h, q;
		
		t = sc.nextInt();
		System.out.println(t);
		
		for(int i = 0; i <= t; i++)
		{
			w = sc.nextInt();
			h = sc.nextInt();
			q = sc.nextInt();
			
			System.out.println(w +h + q);
		}
		

	}

}
