package agenda;

public class Main {

	public static void main(String[] args) {
		Agenda agenda = new Agenda();

		agenda.afegirPersona("Pol Morillas");
		agenda.afegirPersona("Xevier Congost");
		agenda.afegirPersona("Aniol Regincos");
		agenda.afegirPersona("Enric Delgado");
		agenda.afegirPersona("Eric Demeuldre");
		agenda.afegirPersona("Antonio Rodriguez");
		agenda.afegirPersona("Valentin Vasile");
		agenda.afegirPersona("Raul Bueno");
		agenda.afegirPersona("Cameron Scott");
		agenda.afegirPersona("Enric Portas");
		
		agenda.mostrarAgenda();
		agenda.eliminarNom("Enric Portas");
		agenda.mostrarAgenda();
		agenda.eliminarNom("Henry Doors");
	}

}
